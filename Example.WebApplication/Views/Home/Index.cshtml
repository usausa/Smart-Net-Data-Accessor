@{
    ViewData["Title"] = "Home Page";

    var count = (int)ViewBag.Count;
    var list = (IList<DataEntity>)ViewBag.List;
}

<h6>Total count: @count</h6>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Type</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var entity in list)
    {
        <tr>
            <td>@entity.Id</td>
            <td>@entity.Name</td>
            <td>@entity.Type</td>
        </tr>
    }
    </tbody>
</table>
